alcool = 0
gasolina = 0
diesel = 0
while True:
    x = int(input())
    if 0 >= x > 4:
        continue
    else:
        if x == 1:
            alcool += 1
            continue
        elif x == 2:
            gasolina += 1
            continue
        elif x == 3:
            diesel += 1
            continue
        elif x == 4:
            break

print('MUITO OBRIGADO')
print('Alcool:', alcool)
print('Gasolina:', gasolina)
print('Diesel:', diesel)

